{
  "name": "@hlovdal/node-red-display-property",
  "version": "1.1.0",
  "description": "A simple node that displays as its status the value of a message property that passes the node and optional the date/time when the message passed.",
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "prebuild": "del-cli dist && cpy 'src/*.html' dist",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "jest",
    "zzzzz-last": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hlovdal/node-red-display-property.git"
  },
  "main": "dist/display-property.js",
  "author": {
    "name": "Håkon Løvdal",
    "email": "kode@denkule.no"
  },
  "_npmUser": {
    "name": "hlovdal-owner",
    "email": "npm@denkule.no"
  },
  "license": "MIT",
  "node-red": {
    "version": ">=2.0.0",
    "nodes": {
      "display-property": "dist/display-property.js"
    }
  },
  "engines": {
    "node": "*"
  },
  "bugs": {
    "url": "https://github.com/hlovdal/node-red-display-property/issues"
  },
  "keywords": [
    "node-red",
    "property",
    "debug",
    "display",
    "status"
  ],
  "bundleDependencies": [
    "node-red-contrib-typescript-node"
  ],
  "dependencies": {
    "node-red-contrib-typescript-node": "^0.1.0",
    "zzzzz-last-dependencies": "github:AngelsProgram/zzzzz-last-dependency#install"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.0-beta.3",
    "@types/node-red": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "cpy-cli": "^5.0.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "node-red": "^4.0.9",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "zzzzz-last-devdependencies": "github:AngelsProgram/zzzzz-last-dependency#install"
  }
